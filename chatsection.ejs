<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>ExpChats</title>

    <!-- bootstrap -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
    
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.6/dist/umd/popper.min.js"
    integrity="sha384-oBqDVmMz9ATKxIep9tiCxS/Z9fNfEXiDAYTujMAeBAsjFuCZSmKbSSUnQlmh/jp3"
    crossorigin="anonymous"></script>
    
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.min.js"
    integrity="sha384-IDwe1+LCz02ROU9k972gdyvl+AESN10+x7tBKgc9I5HFtuNz0wWnPclzo6p9vxnk"
    crossorigin="anonymous"></script>
    
    <!-- CSS only -->
    <link rel="stylesheet" href="./css/chatstyles.css">
    <!-- fontawesome -->
    
    <script src="https://kit.fontawesome.com/4cacfa305a.js" crossorigin="anonymous"></script>
    
    <!-- friconix -->
    <script defer src="https://friconix.com/cdn/friconix.js"> </script>

    <!-- <script src='/socket.io/socket.io.js' type='text/javascript'></script> -->
    <!-- <script src="/client.js"></script> -->
    <!-- <script src="/socket.io/socket.io.js"></script>
    <script type="text/javascript" src="../js/script.js"></script> -->
</head>

<body>
    <section class="home">
        <!-- nav-bar -->
        <div class="container-fluid">
            <nav class="navbar navbar-expand-lg navbar-dark  navchange">
                <a class="navbar-brand navbrand" href="#"><span class="nbrand">LiCa</span></a>
                <button class="navbar-toggler" type="button" data-toggle="collapse"
                    data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
                    aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav">
                        <li class="lichange nav-item active ">
                            <a class="nav-link" href="#"><span class="brand">Home</span></a>
                        </li>
                        <li class="lichange nav-item active">
                            <a class="nav-link" data-bs-toggle="offcanvas" href="#offcanvasExample" role="button"
                                aria-controls="offcanvasExample">
                                <span class="brand">Account</span>
                            </a>
                        </li>
                    </ul>
                </div>
            </nav>
        </div>
    </section>
    <section class="chatchange container-fluid">

        <!-- offcanvas "profile layover" -->

        <div class="offcanvas offcanvas-end" data-bs-scroll="true" tabindex="-1" id="offcanvasExample"
            aria-labelledby="offcanvasExampleLabel">
            <div class="offcanvas-header">
                <h5 class="offcanvas-title" id="offcanvasExampleLabel">My Profile</h5>
                <button type="button" class="btn-close" data-bs-dismiss="offcanvas" aria-label="Close"></button>
            </div>
            <div class="offcanvas-body" id="profilesec">
                <img src="" alt="">
            </div>
        </div>

        <!-- chatsection -->
        <div class="cardchange card">
            <div class="header p-0 pl-2 card-header">
                <i class="icon fa-brands fa-whatsapp fa-3x"></i>
                <p class="brand"> ExpChats</p>
            </div>
            <div class="messagearea card-body">

            </div>
            <div class="card-footer ">
                <textarea id="typemsg" cols="30" rows="1" placeholder="write a message"></textarea>
            </div>
        </div>
    </section>
